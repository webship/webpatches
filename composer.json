{
  "name": "drupal/webpatches",
  "description": "Modules Patches.",
  "type": "drupal-module",
  "homepage": "https://www.drupal.org/project/webpatches",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Webship",
      "homepage": "http://webship.org",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://www.drupal.org/project/issues/webpatches",
    "source": "http://cgit.drupalcode.org/webpatches"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "composer/installers": "~2.0",
    "oomphinc/composer-installers-extender": "~2.0",
    "cweagans/composer-patches": "~1.0"
  },
  "extra": {
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "locations": {
      "web-root": "web/"
    },
    "patches": {
      "drupal/ace_editor": {
        "Issue #3388322: HTML error when textarea view field has ace format": 
        "web/modules/contrib/webpatches/patches/ace_editor-2023-09-19--3388322-2.patch"
      },
      "drupal/default_content": {
        "Issue #3160146: Add Layout Builder Normalizer and Denormalize": 
        "web/modules/contrib/webpatches/patches/default_content-2021-04-07--3160146-37.patch"
      }
    }
  }
}